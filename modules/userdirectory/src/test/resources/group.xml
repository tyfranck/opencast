<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<group xmlns="http://org.opencastproject.security">
  <id>admin</id>
  <organization id="mh_default_org">
    <name>Opencast Project</name>
    <servers>
      <server name="http://localhost" port="80"/>
    </servers>
    <adminRole>ROLE_ADMIN</adminRole>
    <anonymousRole>ROLE_ANONYMOUS</anonymousRole>
    <properties>
      <property key="logo_large">/admin/img/logo/opencast.svg</property>
      <property key="logo_small">/admin/img/logo/opencast-icon.svg</property>
    </properties>
  </organization>
  <name>Admin</name>
  <description>Admin group</description>
  <role>ROLE_GROUP_ADMIN</role>
  <members>
    <member>admin2</member>
    <member>admin1</member>
  </members>
  <roles>
    <role>
      <name>ROLE_USER</name>
      <organization-id>mh_default_org</organization-id>
    </role>
    <role>
      <name>ROLE_COURSE_ADMIN</name>
      <organization-id>mh_default_org</organization-id>
    </role>
  </roles>
</group>
